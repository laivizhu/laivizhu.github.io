<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
 "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="shopping" extends="struts-base" namespace="/shopping">
        <action name="category_*" class="com.laivi.knowledge.shopping.action.CategoryAction" method="{1}"/>
        <action name="commodity_*" class="com.laivi.knowledge.shopping.action.CommodityAction" method="{1}"/>
        <action name="order_*" class="com.laivi.knowledge.shopping.action.OrderAction" method="{1}"/>
        <action name="shoppingItem_*" class="com.laivi.knowledge.shopping.action.ShoppingItemAction" method="{1}"/>
	</package>
</struts>    
